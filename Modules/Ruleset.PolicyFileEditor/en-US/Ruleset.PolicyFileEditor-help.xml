    <command:details>
      <command:name>Update-GptIniVersion</command:name>
      <command:verb>Update</command:verb>
      <command:noun>GptIniVersion</command:noun>
      <maml:description>
        <maml:para>Increments the version counter in a gpt.ini file.</maml:para>
      </maml:description>
    </command:details>
    <maml:description>
      <maml:para>Increments the version counter in a gpt.ini file.</maml:para>
    </maml:description>
    <command:syntax>
      <command:syntaxItem>
        <maml:name>Update-GptIniVersion</maml:name>
        <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="False" position="1" aliases="none">
          <maml:name>Path</maml:name>
          <maml:description>
            <maml:para>Path to the gpt.ini file that is to be modified.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">System.String</command:parameterValue>
          <dev:type>
            <maml:name>System.String</maml:name>
            <maml:uri />
          </dev:type>
          <dev:defaultValue>None</dev:defaultValue>
        </command:parameter>
        <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="False" position="2" aliases="none">
          <maml:name>PolicyType</maml:name>
